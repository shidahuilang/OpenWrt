
config wifi-device 'ra0'
	option type 'mt_dbdc'
	option hwmode '11g'
	option channel 'auto'
	option htmode 'HT20'
	option country 'CN'
	option txburst '1'
	option smart '1'

config wifi-iface 'default_ra0'
	option device 'ra0'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option key '18080047427'
	option ssid 'OpenWRT-2.4G'
	option ieee80211r '1'

config wifi-device 'rax0'
	option type 'mt_dbdc'
	option hwmode '11a'
	option channel 'auto'
	option htmode 'VHT80'
	option country 'CN'
	option txburst '1'
	option noscan '1'
	option smart '0'

config wifi-iface 'default_rax0'
	option device 'rax0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWRT-5G'
	option encryption 'psk2'
	option key '18080047427'
	option ieee80211r '1'

